drone-data{
	"drones":
	[
	    {
	    "droneID": "integer",
	    "latitude": "float", //latitude of drone's current location
	    "longitude": "float", //longitude of drone's current location
	    "batteryLevel": "float", //battery percentage decimal i.e. .101, .919
	    "status": "string", //shows drone's current status i.e. charging, delivering, returning, waiting
		}
	]
}

EXAMPLE:
drone-data{
	"drones":
	[
	    {
	    "droneID": "14",
	    "latitude": "40.400600", //latitude of drone's current location
	    "longitude": "-80.102951", //longitude of drone's current location
	    "batteryLevel": ".840", //battery percentage decimal i.e. .101, .919
	    "status": "ready", //shows drone's current status i.e. charging, delivering, returning, ready
		},
		{
	    "droneID": "8",
	    "latitude": "40.532600", //latitude of drone's current location
	    "longitude": "-80.412921", //longitude of drone's current location
	    "batteryLevel": ".445", //battery percentage decimal i.e. .101, .919
	    "status": "delivering", //shows drone's current status i.e. charging, delivering, returning, ready
		}
	]
}



