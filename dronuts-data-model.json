customerData{

	"custID": {

		"name":"string",
		"email":"string",
		"password":"string", //salted and hashed pw
		"phonenumber":"string",
		"savedlocation":["latitude","longitude"],
		"payment-token":"string",
		"order-history":["orderID","orderID"],
		"favorites":["donutID","donutID"]

	}
}

